Package: dscoreMSM
Type: Package
Title: Survival Proximity Score Matching in Multi-State Survival Model
Version: 0.1.0
Authors@R: c(person("Atanu", "Bhattacharjee",
                    email = "atanustat@gmail.com",
                    role = c("aut", "cre", "ctb")),
             person("Bhrigu Kumar", "Rajbongshi", role = c("aut", "ctb")),
